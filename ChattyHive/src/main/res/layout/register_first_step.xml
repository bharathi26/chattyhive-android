<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/register_general_padding_top"
    android:paddingBottom="@dimen/register_general_padding_bottom"
    android:background="@drawable/register_general_background"
    android:id="@+id/">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="@dimen/register_general_main_block_with"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:background="#00000000">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/register_main_block_background"
            android:layout_weight="5"
            android:paddingLeft="@dimen/register_general_main_block_padding_left"
            android:paddingTop="@dimen/register_general_main_block_padding_top"
            android:paddingRight="@dimen/register_general_main_block_padding_right"
            android:paddingBottom="@dimen/register_general_main_block_padding_bottom">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/register_first_step_main_block_additional_padding_left"
                android:layout_marginRight="@dimen/register_first_step_main_block_additional_padding_right"
                android:background="#00000000">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/register_first_step_columns_separation"
                    android:background="#00000000">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/register_first_step_name_label"
                        android:id="@+id/textView2"
                        android:layout_marginTop="@dimen/register_first_step_name_text_margin_top"
                        android:layout_marginBottom="@dimen/register_first_step_name_text_margin_bottom"
                        android:textSize="@dimen/register_first_step_name_text"
                        android:textColor="@color/register_first_step_name_label_text"
                        android:background="#00000000" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/register_first_step_name_field_height"
                        android:id="@+id/editText"
                        android:textSize="@dimen/register_first_step_name_field_text"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/register_first_step_name_field_padding_left"
                        android:paddingRight="@dimen/register_first_step_name_field_padding_right"
                        android:layout_marginBottom="@dimen/register_first_step_name_field_margin_bottom"
                        android:hint="@string/register_first_step_name_hint"
                        android:textColor="@color/register_first_step_name_field_text"
                        android:textColorHint="@color/register_first_step_name_field_hint"
                        android:background="@drawable/register_first_step_name_field_background" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/register_first_step_photo_label"
                        android:id="@+id/textView4"
                        android:textSize="@dimen/register_first_step_photo_text"
                        android:textColor="@color/register_first_step_photo_label_text"
                        android:background="#00000000" />

                    <ImageView
                        android:layout_width="@dimen/register_first_step_photo_img_size"
                        android:layout_height="@dimen/register_first_step_photo_img_size"
                        android:id="@+id/imageView2"
                        android:src="@drawable/registro_load_picture"
                        android:layout_marginTop="@dimen/register_first_step_photo_img_margin_top"
                        android:layout_marginBottom="@dimen/register_first_step_photo_img_margin_bottom"
                        android:background="@drawable/register_first_step_photo_background" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="#00000000">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/register_first_step_surname_label"
                        android:id="@+id/textView3"
                        android:layout_marginBottom="@dimen/register_first_step_surname_text_margin_bottom"
                        android:layout_marginTop="@dimen/register_first_step_surname_text_margin_top"
                        android:textSize="@dimen/register_first_step_surname_text"
                        android:textColor="@color/register_first_step_surname_label_text"
                        android:background="#00000000" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/register_first_step_surname_field_height"
                        android:id="@+id/editText2"
                        android:gravity="center_vertical"
                        android:textSize="@dimen/register_first_step_surname_field_text"
                        android:paddingLeft="@dimen/register_first_step_surname_field_padding_left"
                        android:paddingRight="@dimen/register_first_step_surname_field_padding_right"
                        android:layout_marginBottom="@dimen/register_first_step_surname_field_margin_bottom"
                        android:hint="@string/register_first_step_surname_hint"
                        android:background="@drawable/register_first_step_surname_field_background"
                        android:textColor="@color/register_first_step_surname_field_text"
                        android:textColorHint="@color/register_first_step_surname_field_hint" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/register_first_step_birthday_label"
                        android:id="@+id/textView5"
                        android:textSize="@dimen/register_first_step_birthday_text"
                        android:layout_marginBottom="@dimen/register_first_step_birthday_text_margin_bottom"
                        android:textColor="@color/register_first_step_birthday_label_text"
                        android:background="#00000000" />

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/register_first_step_birthday_field_height"
                        android:inputType="date"
                        android:ems="10"
                        android:id="@+id/editText3"
                        android:layout_marginBottom="@dimen/register_first_step_birthday_field_margin_bottom"
                        android:paddingLeft="@dimen/register_first_step_birthday_field_padding_left"
                        android:paddingRight="@dimen/register_first_step_birthday_field_padding_right"
                        android:textSize="@dimen/register_first_step_birthday_field_text"
                        android:gravity="top"
                        android:hint="@string/register_first_step_birthday_hint"
                        android:background="@drawable/abc_textfield_search_default_holo_dark" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/register_first_step_checkbox_margin_bottom"
                        android:background="#00000000">

                        <ImageView
                            android:layout_width="@dimen/register_first_step_checkbox_img_size"
                            android:layout_height="@dimen/register_first_step_checkbox_img_size"
                            android:id="@+id/imageView3"
                            android:src="@drawable/register_checkbox_button"
                            android:scaleType="centerInside"
                            android:adjustViewBounds="true"
                            android:layout_marginRight="@dimen/register_first_step_checkbox_img_separation"
                            android:background="#00000000" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/register_first_step_checkbox_label"
                            android:id="@+id/textView6"
                            android:layout_gravity="center_vertical"
                            android:textSize="@dimen/register_first_step_checkbox_text"
                            android:textColor="@color/register_first_step_checkbox_text"
                            android:background="#00000000" />
                    </LinearLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/register_first_step_gender_label"
                        android:id="@+id/textView7"
                        android:textSize="@dimen/register_first_step_gender_text"
                        android:layout_marginBottom="@dimen/register_first_step_gender_text_margin_bottom"
                        android:textColor="@color/register_first_step_gender_label_text"
                        android:background="#00000000" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/register_first_step_radio_button_margin_bottom"
                        android:background="#00000000">

                        <ImageView
                            android:layout_width="@dimen/register_first_step_radio_button_img_size"
                            android:layout_height="@dimen/register_first_step_radio_button_img_size"
                            android:id="@+id/imageView4"
                            android:src="@drawable/register_radio_button"
                            android:scaleType="centerInside"
                            android:adjustViewBounds="true"
                            android:layout_marginRight="@dimen/register_first_step_radio_button_img_separation"
                            android:background="#00000000" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/register_first_step_gender_male"
                            android:id="@+id/textView8"
                            android:layout_marginRight="@dimen/register_first_step_radio_button_separation"
                            android:textSize="@dimen/register_first_step_radio_button_text"
                            android:layout_gravity="center_vertical"
                            android:textColor="@color/register_first_step_gender_radio_button_text"
                            android:background="#00000000" />

                        <ImageView
                            android:layout_width="@dimen/register_first_step_radio_button_img_size"
                            android:layout_height="@dimen/register_first_step_radio_button_img_size"
                            android:id="@+id/imageView5"
                            android:src="@drawable/register_radio_button"
                            android:scaleType="centerInside"
                            android:adjustViewBounds="true"
                            android:layout_marginRight="@dimen/register_first_step_radio_button_img_separation"
                            android:background="#00000000" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/register_first_step_gender_female"
                            android:id="@+id/textView9"
                            android:minLines="1"
                            android:maxLines="1"
                            android:lines="1"
                            android:textSize="@dimen/register_first_step_radio_button_text"
                            android:layout_gravity="center_vertical"
                            android:textColor="@color/register_first_step_gender_radio_button_text"
                            android:background="#00000000" />
                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/register_first_step_main_block_additional_padding_right"
                android:layout_gravity="center_vertical"
                android:layout_marginBottom="@dimen/register_first_step_location_margin_bottom"
                android:background="#00000000">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_first_step_location_label"
                    android:id="@+id/textView10"
                    android:textSize="@dimen/register_first_step_location_text"
                    android:layout_marginLeft="@dimen/register_first_step_main_block_additional_padding_left"
                    android:textColor="@color/register_first_step_location_label_text"
                    android:background="#00000000" />

                <ImageView
                    android:layout_width="@dimen/register_first_step_location_img_size"
                    android:layout_height="@dimen/register_first_step_location_img_size"
                    android:id="@+id/imageView6"
                    android:src="@drawable/registro_localizacion1"
                    android:adjustViewBounds="true"
                    android:scaleType="centerInside"
                    android:layout_marginLeft="@dimen/register_first_step_location_img_separation_left"
                    android:layout_marginRight="@dimen/register_first_step_location_img_separation_right"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_first_step_location_hint"
                    android:id="@+id/textView11"
                    android:textSize="@dimen/register_first_step_location_field_text"
                    android:textColor="@color/register_first_step_location_field_text"
                    android:background="#00000000" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/register_first_step_main_block_additional_padding_right"
                android:layout_gravity="center_vertical"
                android:background="#00000000">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_first_step_languages_label"
                    android:id="@+id/textView12"
                    android:textSize="@dimen/register_first_step_languages_text"
                    android:layout_marginRight="@dimen/register_first_step_languages_separation"
                    android:layout_marginLeft="@dimen/register_first_step_main_block_additional_padding_left"
                    android:textColor="@color/register_first_step_languages_label_text"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_first_step_languages_hint"
                    android:id="@+id/textView13"
                    android:textSize="@dimen/register_first_step_languages_field_text"
                    android:textColor="@color/register_first_step_languages_field_text"
                    android:background="#00000000" />
            </LinearLayout>

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00000000"></FrameLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginBottom="@dimen/register_first_step_private_note_margin_bottom"
                android:layout_marginLeft="@dimen/register_first_step_private_note_margin_left"
                android:layout_marginRight="@dimen/register_first_step_private_note_margin_right"
                android:background="#00000000">

                <ImageView
                    android:layout_width="@dimen/register_first_step_private_note_img_size"
                    android:layout_height="@dimen/register_first_step_private_note_img_size"
                    android:id="@+id/imageView"
                    android:src="@drawable/registro_private"
                    android:adjustViewBounds="true"
                    android:baselineAlignBottom="false"
                    android:clickable="false"
                    android:cropToPadding="false"
                    android:scaleType="centerInside"
                    android:layout_marginRight="@dimen/register_first_step_private_note_img_separation"
                    android:background="#00000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/register_first_step_private_note"
                    android:id="@+id/textView"
                    android:textSize="@dimen/register_first_step_private_note_text"
                    android:textColor="@color/register_first_step_private_note_text"
                    android:background="#00000000" />
            </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/register_general_buttons_margin_top"
            android:background="#00000000">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/register_first_step_buttons_back"
                android:id="@+id/button"
                android:background="@drawable/register_buttons_back_background"
                android:maxWidth="@dimen/register_general_buttons_max_with"
                android:layout_gravity="left"
                android:width="@dimen/register_general_buttons_max_with"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:textColor="@color/register_general_buttons_back_text"
                android:paddingLeft="@dimen/register_general_buttons_padding_left"
                android:paddingTop="@dimen/register_general_buttons_padding_top"
                android:paddingRight="@dimen/register_general_buttons_padding_right"
                android:paddingBottom="@dimen/register_general_buttons_padding_bottom"
                android:textSize="@dimen/register_general_buttons_text_size" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/register_first_step_buttons_next"
                android:id="@+id/button2"
                android:background="@drawable/register_buttons_next_background"
                android:maxWidth="@dimen/register_general_buttons_max_with"
                android:width="@dimen/register_general_buttons_max_with"
                android:layout_gravity="right"
                android:layout_alignParentLeft="false"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:textColor="@color/register_general_buttons_next_text"
                android:paddingLeft="@dimen/register_general_buttons_padding_left"
                android:paddingTop="@dimen/register_general_buttons_padding_top"
                android:paddingRight="@dimen/register_general_buttons_padding_right"
                android:paddingBottom="@dimen/register_general_buttons_padding_bottom"
                android:textSize="@dimen/register_general_buttons_text_size" />
        </RelativeLayout>
    </LinearLayout>
</FrameLayout>